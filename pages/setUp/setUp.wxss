/* pages/setUp/setUp.wxss */

.setUp {
  width: 100%;
  height: 100%;
  background-color: #fff;
  padding: 220rpx 0 0 0;
  box-sizing: border-box;
}
.setUp .setUp_box{
  padding:  0 40rpx;
  box-sizing: border-box;
}
.setUp .setUp_function {
  padding: 30rpx 0;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
  justify-content: space-between;
}

.setUp_function .setUp_function_describe {
  font-size: 28rpx;
  color: #989899;
  display: flex;
  align-items: center;
}

.setUp_function_describe .setUp_function_describe_size {
  margin-left: 10rpx;
}

.setUp .setUp_function:last-child {
  padding-top: 30rpx;
  padding-bottom: 0;
  border: 0;
}
